<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.psyburger6">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Psyburger6">
        <activity
            android:name=".log_activity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".FindNextSingerActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".TagCardActivity" />
        <activity android:name=".SelectSongActivity" />
        <activity android:name=".PlaySongActivity" />
        <activity android:name=".HappyResultActivity" />
        <activity android:name=".SadResultActivity" />
        <activity android:name=".RecomSongActivity" />
        <activity android:name=".CallWhoActivity" />
        <activity android:name=".EnergyActivity" />

        <meta-data
            android:name="com.robotemi.sdk.metadata.SKILL"
            android:value="@string/app_name" />
    </application>

</manifest>