<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/karaoback">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <WebView
            android:id="@+id/webView"
            android:layout_width="match_parent"
            android:layout_height="687dp"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/gohome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="710dp"
            android:layout_marginLeft="50dp"
            android:background="#00000000"
            android:scaleType="fitXY"
            android:src="@drawable/gohome" />

        <Button
            android:id="@+id/happysong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="700dp"
            android:text="신나는 노래를 불렀을 때"/>

        <Button
            android:id="@+id/dance_OnOff"
            android:layout_width="160dp"
            android:layout_height="109dp"
            android:layout_marginLeft="570dp"
            android:layout_marginTop="700dp"
            android:text="춤안추기"
            android:textSize="30sp" />

        <Button
            android:id="@+id/sadsong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="800dp"
            android:layout_marginTop="700dp"
            android:text="우울한 노래를 불렀을 때"/>



    </FrameLayout>


</LinearLayout>